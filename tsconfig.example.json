{
  "compilerOptions": {
    "target": "ES2020",
    "module": "Node16",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "strict": true,
    "baseUrl": "./",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [
    "example-generics.ts",
    "src/**/*"
  ]
}